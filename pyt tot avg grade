sno=int(input("enter the sno"))
sname=input("enter the std name")
grup=input("enter the std group")
s1=int(input("enter the sub1"))
s2=int(input("enter the sub2"))
s3=int(input("enter the sub3"))
tot=s1+s2+s3
avg=tot/3
if(avg>=90):
    print("O-grade")
elif(avg>=80):
    print("A-grade")
elif(avg>=70):
    print("B-grade")
elif(avg>=60):
    print("C-grade")
elif(avg>=50):
    print("D-grade")
else:
    print("pass")
