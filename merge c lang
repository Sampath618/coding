#include<stdio.h>
#include<conio.h>
int main()
{
	int a[100],b[100],e[100];
	int i,ind=0,r,c;
	scanf("%d %d",&r,&c);
	{
	for(i=0;i<r;i++)
	scanf("%d",&a[i]);
}
for(i=0;i<c;i++){
	scanf("%d",&b[i]);
}
for(i=0;i<r;i++){
	e[ind]=a[i];
	ind++;
}
	for(i=0;i<c;i++){
	e[ind]=b[i];
	ind++;
}
	for(i=0;i<(r+c);i++)
	{
		printf("%d",e[i]);
	}
	return 0;
}
